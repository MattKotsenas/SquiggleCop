            {
              "id": "CA1707",
              "shortDescription": {
                "text": "Identifiers should not contain underscores"
              },
              "fullDescription": {
                "text": "By convention, identifier names do not contain the underscore (_) character. This rule checks namespaces, types, members, and parameters."
              },
