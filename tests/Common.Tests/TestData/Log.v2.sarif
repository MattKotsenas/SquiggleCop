              "shortDescription": {
                "text": "Use \"DateTimeOffset\" instead of \"DateTime\""
              },
              "fullDescription": {
                "text": "This rule recommends using DateTimeOffset instead of DateTime for projects targeting .NET Framework 2.0 or later."
              },
              "defaultConfiguration": {
