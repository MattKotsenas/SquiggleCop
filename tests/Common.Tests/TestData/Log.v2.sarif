            },
            {
              "id": "CA1870",
              "shortDescription": {
                "text": "Use a cached 'SearchValues' instance"
              },
              "fullDescription": {
                "text": "Using a cached 'SearchValues' instance is more efficient than passing values to 'IndexOfAny'/'ContainsAny' directly."
